or: #333333;\n  position: absolute;\n  color: white;\n  min-width: 160px;\n  box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);\n  z-index: 1;\n  margin-top: 55px;\n  right: 0;\n}\nnav > div > button:hover + .dropdown-content {\n  display: block;\n}\n.dropdown-content a {\n  display: block;\n  text-align: right;\n  color: white;\n  text-decoration: none;\n}\n.dropdown-content a:hover {\n  background-color: #ffffff;\n}\n\n/* BREAKPOINTS*\n  / *====================================== */\n@media screen and (min-width: 768px) {\n  .homePage {\n    height: 400px;\n  }\n  nav > div .nav {\n    width: 35vw;\n  }\n  nav > div .nav a {\n    padding: 15px 19px;\n  }\n  nav .logo {\n    width: 80%;\n  }\n  .homePage {\n    background: url(../assets/images/hiking3.png) no-repeat -462px -243px;\n  }\n}\n\n@media (min-width: 992px) {\nnav {\n  height:4vh;\n}\n  nav > div .nav a {\n    padding: 15px 35px;\n  }\n  .homePage {\n    background: url(../assets/images/hiking3.png) no-repeat -443px -245px;\n  }\n}\n\n@media (min-width: 1200px) {\n  nav > div .nav {\n    width: 25vw;\n  }\n  nav > div .nav a {\n    padding: 15px 29px;\n  }\n  nav .logo {\n    width: 60%;\n  }\n}\n@media (min-width: 1500px){\n  .homePage {\n    height: 650px;\n      background: url(../assets/images/hiking3.png)no-repeat 0px 0px;\n        background-size:100% 100%;\n  }\nnav > div .nav {\n    width: 19vw;\n}\n\n}\n@media (min-width: 2000px) {\nnav > div .nav {\n    width: 14vw;\n}\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}